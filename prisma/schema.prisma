generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  username  String @unique
  password String 
  email String
}

model Form {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  name String @unique
  age Int
  school String
  call Int
  clip String
  url String
}