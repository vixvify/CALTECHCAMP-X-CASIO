generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  team String  
  school String
  email String @unique
  call1 String
  call2 String
  relation String
  name1 String 
  name2 String 
  name3 String 
  name4 String 
  name5 String 
  url String
  clip String
  qi1 String
  qm1 String
  qm2 String
  qa1 String
  qp1 String
  username  String @unique
  password String? 
  stats String
}
