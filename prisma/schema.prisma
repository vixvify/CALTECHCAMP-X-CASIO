generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  team String  @unique
  school String
  email String @unique
  call String
  name1 String 
  name2 String 
  name3 String 
  url String
  clip String
  username  String @unique
  password String? 
  stats String
}
